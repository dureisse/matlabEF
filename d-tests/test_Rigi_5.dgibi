* Test de l'element 3D CUB8
OPTI DIME 2 ELEM TRI3;
P1 = 0. 0.; 
P2 = 2. 0.;
P3 = 3. 1.;
P4 = 0. 2.;
M1 = MANU QUA4 P1 P2 P3 P4;
OPTI DIME 3 ELEM CUB8;
MAIL1 = M1 VOLU TRAN 1 (0. 0. 1.);
MODL1 = MODE MAIL1 MECANIQUE ELASTIQUE;
MATR1 = MATE MODL1 YOUNG 1. NU 0.3;
RIGI1 = RIGI MODL1 MATR1;
X Y Z = COOR MAIL1;
X = (X ** 3) + Y;
Y = Y - X;
C1 = (EXCO SCAL X UX NATURE DIFFUS) ET
     (EXCO SCAL Y UY NATURE DIFFUS);
XVAL1 = XTMX C1 RIGI1;
list (XVAL1);
*list (XVAL1 - 1.3461538461);
*list (XVAL1 - 50.000);
list (XVAL1 - 352.27272727);
MESS 'XVAL1' XVAL1;

OPTI SORT 'test_Rigi_5';
SORT AVS MAIL1 C1 MATR1;
OPTI SORT 'poub';




FIN;

