* Test du raccord de poutres
OPTI DIME 2 ELEM SEG2;

P1 = 0. 0.; P2 = 1. 0.; P3 = 2. 0.5;
L1 = P1 D 1 P2 D 1 P3;

MODL1 = MODE L1 MECANIQUE ELASTIQUE POUT;
MATR1 = MATE MODL1 YOUN 1. NU 0. SECT 1. INRZ 1.;
RIGI1 = RIGI MODL1 MATR1;

U1 = COOR 1 L1;
U2 = COOR 2 L1;
XX = (EXCO SCAL (U1 * U2) UX NATURE DIFFUS) ET
     (EXCO SCAL (U2 * U2) UY NATURE DIFFUS);
ERR1 = XTMX RIGI1 XX;
list (ERR1 - 1.3349325825);


OPTI SORT 'test_Pout_3';
SORT AVS L1 MATR1 XX;
OPTI SORT 'poub';

FIN;
