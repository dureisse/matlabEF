* On teste les QUA8
OPTI DIME 2 ELEM QUA8;
OPTI MODE PLAN CONT;
P1 = -1. -1.; P2 = 1. -0.8; P3 = 0. 2.;
L1 = P1 D 3 P2; M1 = L1 TRAN 2 P3;
*L1 = P1 D 1 P2; M1 = L1 TRAN 1 P3;

X Y = COOR M1;
C1 = ((EXCO SCAL X UX) ** 2) ET (EXCO SCAL Y UY);

MODL1 = MODE M1 MECANIQUE ELASTIQUE;
MATR1 = MATE MODL1 RHO 100.;
MASS1 = MASS MODL1 MATR1;
XVAL1 = XTMX C1 MASS1;
*list (XVAL1 - 213.33333333);
list (XVAL1 - 218.6666666);
MESS 'XVAL1' XVAL1;

U1 = MANU CHPO M1 2 'UX' 1. 'UY' 1. NATURE DIFFUS;
DEUXM1 = XTMX U1 MASS1;
MESS 'DEUXM1' DEUXM1;

OPTI SORT 'test_Mass_1b';
SORT AVS M1 C1;
OPTI SORT 'poub';

FIN;
